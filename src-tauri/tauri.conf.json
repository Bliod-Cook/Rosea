{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "Rosea",
  "version": "0.2.0",
  "identifier": "Rosea",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm run web:dev",
    "beforeBuildCommand": "pnpm run web:build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Rosea",
        "width": 136,
        "height": 52,
        "resizable": false,
        "fullscreen": false,
        "decorations": false,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "transparent": true,
        "shadow": false,
        "focus": false,
        "contentProtected": true,
        "x": 0,
        "y": 0,
        "zoomHotkeysEnabled": false,
        "maximizable": false
      },
      {
        "label": "canvas",
        "title": "Canvas",
        "url": "/canvas/",
        "x": 0,
        "y": 0,
        "width": 400,
        "height": 400,
        "transparent": true,
        "alwaysOnTop": true,
        "decorations": false,
        "shadow": false,
        "resizable": false,
        "skipTaskbar": true,
        "visible": true,
        "zoomHotkeysEnabled": false,
        "maximizable": true,
        "minimizable": false
      },
      {
        "label": "menu",
        "title": "Menu",
        "parent": "canvas",
        "url": "/menu/",
        "width": 40,
        "height": 40,
        "transparent": true,
        "alwaysOnTop": true,
        "decorations": false,
        "shadow": false,
        "resizable": false,
        "skipTaskbar": true,
        "visible": false,
        "zoomHotkeysEnabled": false,
        "maximizable": false
      },
      {
        "label": "random",
        "url": "/randomer/",
        "title": "Randomer",
        "width": 120,
        "height": 180,
        "transparent": true,
        "alwaysOnTop": true,
        "decorations": false,
        "shadow": false,
        "resizable": false,
        "skipTaskbar": true,
        "visible": false,
        "zoomHotkeysEnabled": false,
        "maximizable": false
      },
      {
        "label": "random/settings",
        "url": "/randomer/settings/",
        "title": "Randomer Settings",
        "width": 320,
        "height": 230,
        "transparent": true,
        "alwaysOnTop": true,
        "decorations": false,
        "shadow": false,
        "resizable": false,
        "skipTaskbar": true,
        "visible": false,
        "zoomHotkeysEnabled": false,
        "fullscreen": false,
        "maximizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32.jpg",
      "icons/128.jpg",
      "icons/64.jpg",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDM0MTYzNzI3RTJENkMyNjkKUldScHd0YmlKemNXTkkwZFJHSHV1UThtWmRENTBwUW8yRWhCbzBBbDFYL2F1REoza1hqNHNaTjQK",
      "endpoints": [
        "https://proxy.rosea.planten-dev.top",
        "https://github.com/Bliod-Cook/Rosea/releases/latest/download/latest.json"
      ]
    }
  }
}
